<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.kpfu.itis.bagaviev.taskbot.controlllers.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TextArea fx:id="taChat" prefHeight="200.0" prefWidth="200.0" />
      <TextField fx:id="tfCommand" promptText="Введите команду" />
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0" style="-fx-spacing: 16;">
            <Button mnemonicParsing="false" onAction="#onRunButtonClick" text="Отправить" />
      </HBox>
   </children>
</VBox>
